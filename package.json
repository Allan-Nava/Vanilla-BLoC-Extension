{
	"name": "vanilla-bloc",
	"displayName": "Vanilla BLoC",
	"description": "Support for the BLoC command allows you to create a Vanilla BLoC and the Events/States",
	"version": "0.0.10",
	"publisher": "Allan-Nava",
	"bugs": {
		"url": "https://github.com/Allan-Nava/Vanilla-BLoC-Extension/issues",
		"email": "allannava95@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Allan-Nava/Vanilla-BLoC-Extension"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"keywords": [
		"dart",
		"flutter",
		"angulardart",
		"bloc",
		"state-management"
	],
	"activationEvents": [
		"onCommand:vanilla-bloc.helloWorld",
		"onCommand:vanilla-bloc.new-bloc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vanilla-bloc.new-bloc",
				"title": "Vanilla BLoC: New Bloc"
			},
			{
				"command": "vanilla-bloc.new-snapshot",
				"title": "Vanilla BLoC: New Snapshot"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "vanilla-bloc.new-bloc",
					"group": "blocGroup@1",
					"when": "explorerResourceIsFolder"
				},
				{
					"command": "vanilla-bloc.new-snapshot",
					"group": "blocGroup@1",
					"when": "explorerResourceIsFolder"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/lodash": "^4.14.152",
		"@types/mkdirp": "^1.0.0",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.8",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/change-case": "^2.3.1"
	}
}
